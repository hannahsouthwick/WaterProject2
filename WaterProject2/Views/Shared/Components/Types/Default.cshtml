@{
}

@model IEnumerable<string>

<div class="d-grid gap-2">
    <a asp-controller="Home" asp-action="Index" asp-route-projectType=" " asp-route-pageNum = "1" class="btn btn-outline-secondary">Home</a>

    @foreach (string t in Model)
    {
        @*shortcut for if statement determining button appearances*@ 
        <a asp-controller="Home" asp-action="Index" asp-route-projectType="@t"
           class="btn @(ViewBag.SelectedType == t ? "btn-primaray" : "btn-outline-secondary")">@t</a>
    }
</div>